<html>
    <head>
        <title></title>
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" 
        integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    </head>
<body>
    
   <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top" style="opacity:0.9; ">
      <a class="navbar-brand" href="/">MoVieS PoInT</a>
        <form class="form-inline my-2 my-lg-0" action="/res" method="GET">
          <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search" name="movie">
          <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
        </form>
      </div>
</nav>
    


    <div class="container">
             <div class="row">
         <% data["Search"].forEach((movies)=>{ %>
                 
               
                    <div class="col-sm">
                        <div class="card" style="width: 18rem ;">
                         <img class="card-img-top" src=<%= movies.Poster %> alt="Card image cap">
                          <div class="card-body">
                                <p class="card-title"> <%= movies.Title %></p>
                           </div>
                         </div>
                            
                    </div>

                
                   
        
          <% }); %>
          </div>
        </div>
    </div>

    
</body>
</html>